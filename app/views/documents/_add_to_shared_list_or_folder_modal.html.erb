          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <%= render 'add_to_shared_list_form', document: document %>
        <%= render 'add_to_folder_form', document: document  %>
      </div>
    </div>
  </div>
</div>
