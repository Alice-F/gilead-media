<!-- Modal -->
<div class="modal fade" id="addFolder<%= folder.id %>ToSharedList" tabindex="-1" role="dialog" aria-labelledby="addFolder<%= folder.id %>ToSharedListTitle" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="addFolder<%= folder.id %>ToSharedListTitle">Ajouter <%= folder.title %> Ã </h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <%= render 'add_to_shared_list_form', folder: folder %>
      </div>
    </div>
  </div>
</div>
